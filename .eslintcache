     <label>{label}</label>\n        <input\n          name={name}\n          value={formData[name] || \"\"}\n          type={type || \"text\"}\n          onChange={handleOnChange}\n        />\n      </div>\n    );\n  }\n  function handleOnChange(e) {\n    const name = e.target.name;\n    const value = e.target.value;\n    setFormData({ ...formData, [name]: value });\n  }\n\n  return (\n    <div>\n      <NavBar />\n      <h1>Update Customer</h1>\n      <form onSubmit={handleOnSubmit}>\n        {renderInput(\"name\", \"Customer Name\")}\n        {renderInput(\"email\", \"Customer Email\", \"email\")}\n        {renderInput(\"organisationNr\", \"Organization Number\")}\n        {renderInput(\"paymentTerm\", \"Payment Term\", \"number\")}\n        {renderInput(\"phoneNumber\", \"Phone Number\", \"tel\")}\n        {renderInput(\"reference\", \"Reference\")}\n        {renderInput(\"vatNr\", \"Vat Number\")}\n        {renderInput(\"website\", \"Website\", \"url\")}\n        <button type=\"submit\">Update Customer</button>\n      </form>\n    </div>\n  );\n}\n","/Users/OzgurErna/Desktop/WebDevelopment/Frontend-3/frontend3-assignment/src/pages/CustomerDetailPage.jsx",[],"/Users/OzgurErna/Desktop/WebDevelopment/Frontend-3/frontend3-assignment/src/styles/HeadingStyled.jsx",[],"/Users/OzgurErna/Desktop/WebDevelopment/Frontend-3/frontend3-assignment/src/components/NavBar.jsx",[],"/Users/OzgurErna/Desktop/WebDevelopment/Frontend-3/frontend3-assignment/src/context/UserContext.js",["57"],"import React, { createContext } from \"react\";\n\nexport const UserContext = createContext();\n",{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","severity":1,"message":"63","line":2,"column":25,"nodeType":"64","messageId":"65","endLine":2,"endColumn":29},{"ruleId":"62","severity":1,"message":"66","line":15,"column":9,"nodeType":"64","messageId":"65","endLine":15,"endColumn":24},{"ruleId":"62","severity":1,"message":"67","line":1,"column":8,"nodeType":"64","messa